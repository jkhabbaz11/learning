#define _CRT_SECURE_NO_WARNINGS
#define _CRT_NONSTDC_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#include <string.h>


long long card;
long long askCard()
{
	
	printf("Enter your card number here: ");
	scanf("%lld", &card);
	
	return card;
}
char * convertToArray()
{
	char cardArray[256];
	sprintf(cardArray, "%lld", askCard());
	return cardArray;

}

int compareToCards()
{
	switch (strlen(convertToArray()))
	{
	case 15:printf("Amex");
		char amEx1[1] = { 34 };
		char amEx2[1] = { 37 };
		printf("%lld", card % 100);



		break;
	case 16: printf("visa or master");
		char visa[1] = { 4 };
		char mc1[1] = { 51 };
		char mc2[1] = { 52 };
		char mc3[1] = { 53 };
		char mc4[1] = { 54 };
		char mc5[1] = { 55 };




		break;
	case 13: printf("visa");
		char visa[1] = { 4 };




		break;
	}
	return 0;
	
	
}



int main(void) {
	compareToCards();

	

	return 0;

	
	
}
