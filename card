#define _CRT_SECURE_NO_WARNINGS
#define _CRT_NONSTDC_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#include <string.h>



long long askCard()
{
	long long card;
	printf("Enter your card number here: ");
	scanf("%lld", &card);
	
	return card;
}
char * convertToArray()
{
	char cardArray[256];
	sprintf(cardArray, "%lld", askCard());
	printf("%", cardArray);
	return cardArray;

}

int compareAmEx()
{
	char amEx1[1] = { 34 };
	char amEx2[1] = { 37 };
	if (strlen(convertToArray()) == 15)
	{	
		printf("%d", strlen(convertToArray()));
		printf("hi");
	}
	return 0;
}



int main(void) {
	convertToArray();
	compareAmEx();

	

	return 0;

	
	
}
