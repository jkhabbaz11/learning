#define _CRT_SECURE_NO_WARNINGS
#define _CRT_NONSTDC_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#include <string.h>





int compareToCards(char * cardArray)
{
	switch (strlen(cardArray))
	{
	case 15:
	{	
	char amEx1[1] = {34};
	char amEx2[1] = {37};
	printf("%s",cardArray);
	printf("it is a american express 1 %d\n", strncmp(amEx1, cardArray, 2));
	printf("it is a american express 2 %d\n", strncmp(amEx2, cardArray, 2));
	


	break; }
	case 16: printf("visa or master");
	{
		char visa[1] = { 4 };
		char mc1[1] = { 51 };
		char mc2[1] = { 52 };
		char mc3[1] = { 53 };
		char mc4[1] = { 54 };
		char mc5[1] = { 55 };




		break; }
	case 13: printf("visa");
	{	char visa[1] = { 4 };




		break;
	}
	}
	return 0;
	
	
}



int main(void) {
	long long card;
	printf("Enter your card number here: ");
	scanf("%lld", &card);
	char cardArray[256];
	sprintf(cardArray, "%lld", card);
	compareToCards(cardArray);

	

	return 0;

	
	
}
